{
  "name": "@trixis/lib-ts-bot",
  "version": "0.1.16",
  "description": "Package for Discord bots micro-framework template - https://github.com/TrixiS/base-ts-bot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "publish": "rm ./dist -rf && pnpm tsc && pnpm version patch && pnpm publish --access=public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./checks": "./dist/checks/index.js",
    "./extensions": "./dist/extensions/index.js",
    "./utils": "./dist/utils/index.js"
  },
  "devDependencies": {
    "@types/node": "^18.7.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "discord.js": "^14.2.0"
  }
}
